{
  "name": "@tetherto/wdk-protocol-bridge-usdt0-evm",
  "version": "1.0.0-beta.1",
  "description": "A simple package that lets @tetherto/wdk-wallet-evm and wallet-evm-erc-4337 wallet accounts interact with the usdt0 protocol.",
  "keywords": [
    "wdk",
    "protocol",
    "bridge",
    "usdt0",
    "ethereum",
    "erc-4337"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-protocol-bridge-usdt0-evm.git"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@layerzerolabs/lz-v2-utilities": "3.0.114",
    "@tetherto/wdk-wallet": "^1.0.0-beta.1",
    "@tetherto/wdk-wallet-evm": "^1.0.0-beta.2",
    "@tetherto/wdk-wallet-evm-erc-4337": "^1.0.0-beta.1",
    "@ton/core": "0.61.0",
    "bare-wdk-runtime": "2.0.1",
    "ethers": "6.13.5",
    "tronweb": "6.0.3"
  },
  "devDependencies": {
    "cross-env": "10.0.0",
    "jest": "30.0.5",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "overrides": {
    "axios": "1.12.2",
    "cookie": "1.0.2",
    "tmp": "0.2.5",
    "ws": "8.18.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/"
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
